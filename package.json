{
  "name": "@tauri-apps/tauricon",
  "version": "1.0.3",
  "description": "Make icons for your Tauri app.",
  "bin": {
    "tauricon": "./bin/tauricon.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tauri-apps/tauricon.git"
  },
  "license": "Apache-2.0 OR MIT",
  "bugs": {
    "url": "https://github.com/tauri-apps/tauricon/issues"
  },
  "homepage": "https://github.com/tauri-apps/tauricon#readme",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/tauri"
  },
  "files": [
    "bin",
    "dist",
    "scripts",
    "src/helpers/icns.json"
  ],
  "scripts": {
    "postinstall": "",
    "build": "rimraf ./dist && rollup -c --silent",
    "build-release": "rimraf ./dist && cross-env NODE_ENV=production rollup -c",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --forceExit --no-cache",
    "pretest": "yarn build",
    "prepublishOnly": "yarn build-release",
    "lint": "eslint --ext ts \"./src/**/*.ts\"",
    "lint-fix": "eslint --fix --ext ts \"./src/**/*.ts\"",
    "lint:lockfile": "lockfile-lint --path yarn.lock --type yarn --validate-https --allowed-hosts npm yarn",
    "format": "prettier --write --end-of-line=auto \"./src/**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check --end-of-line=auto \"./src/**/*.{cjs,js,jsx,ts,tsx,html,css,json}\" --ignore-path .gitignore"
  },
  "contributors": [
    "Tauri Programme within The Commons Conservancy"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 12.13.0",
    "npm": ">= 6.6.0",
    "yarn": ">= 1.19.1"
  },
  "dependencies": {
    "@fiahfy/icns": "0.0.7",
    "chalk": "5.3.0",
    "cross-env": "7.0.3",
    "cross-spawn": "7.0.3",
    "find-up": "7.0.0",
    "fs-extra": "11.2.0",
    "glob": "10.4.1",
    "global-agent": "3.0.0",
    "got": "14.4.1",
    "imagemin": "9.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-zopfli": "7.0.0",
    "inquirer": "9.2.23",
    "is-png": "3.0.1",
    "minimist": "1.2.8",
    "ms": "2.1.3",
    "png-to-ico": "2.1.8",
    "read-chunk": "4.0.3",
    "semver": "7.6.2",
    "sharp": "0.33.4",
    "update-notifier": "7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "26.0.1",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-typescript": "11.1.6",
    "@types/cross-spawn": "6.0.6",
    "@types/fs-extra": "11.0.4",
    "@types/glob": "8.1.0",
    "@types/global-agent": "2.1.3",
    "@types/imagemin": "9.0.0",
    "@types/imagemin-optipng": "5.2.4",
    "@types/inquirer": "9.0.7",
    "@types/ms": "0.7.34",
    "@types/semver": "7.5.8",
    "@types/sharp": "0.31.1",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "babel-jest": "29.7.0",
    "covector": "0.11.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-lodash-template": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.2.0",
    "eslint-plugin-security": "3.0.1",
    "is-running": "2.1.0",
    "jest": "29.7.0",
    "jest-transform-toml": "1.0.0",
    "lockfile-lint": "4.14.0",
    "prettier": "3.3.2",
    "promise": "8.3.0",
    "rimraf": "5.0.7",
    "rollup": "4.18.0",
    "rollup-plugin-terser": "7.0.2",
    "tslib": "2.6.3",
    "typescript": "5.4.5"
  },
  "resolutions": {
    "**/semver-regex": "4.0.3",
    "**/trim-newlines": "4.0.2"
  }
}
